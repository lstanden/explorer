{
  "name": "phylogeny-explorer",
  "version": "2.0.0",
  "description": "Phylogeny Explorer",
  "private": true,
  "workspaces": [
    "client",
    "common",
    "daemon",
    "tree-api",
    "user-api"
  ],
  "scripts": {
    "seed": "sh tools/seed.sh",
    "dev": "npm-run-all -p dev:*",
    "dev:daemon": "npm run --prefix daemon dev",
    "dev:tree-api": "npm run --prefix tree-api dev",
    "dev:user-api": "npm run --prefix user-api dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phylogeny-explorer/explorer.git"
  },
  "author": "Phylogeny Explorer Project",
  "license": "ISC",
  "devDependencies": {
    "mongoimport": "^1.0.6"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
